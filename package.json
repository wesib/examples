    "build:bundle": "rollup --config ./rollup.config.js",
    "build:copy-assets": "node ./build/copy-assets.js",
    "clean": "node ./build/clean",
    "lint": "eslint . --ext .js,.ts",
    "rebuild": "run-s clean build",
    "site:gh-pages": "gh-pages --dist dist --dotfiles",
    "site:publish": "run-s all \"site:gh-pages {@}\" --",
    "start": "http-server ./dist -d false -p 4200"
