    "@run-z/eslint-config": "^3.3.1",
    "@run-z/log-z": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@wesib/css": "^2.0.2",
    "@wesib/forms": "^2.0.2",
    "@wesib/generic": "^2.0.2",
    "@wesib/navigation": "^2.0.2",
    "@wesib/wesib": "^2.0.2",
    "eslint": "^8.36.0",
    "gh-pages": "^5.0.0",
    "prettier": "^2.8.6",
    "prettier-eslint-cli": "^7.1.0",
    "rollup": "^3.20.1",
    "run-z": "^1.11.0",
    "shx": "^0.3.4",
    "tslib": "^2.5.0",
    "typescript": "~4.9.5",
    "vite": "^2.9.15",
    "vite-plugin-html": "^2.1.2"
  },
  "scripts": {
    "all": "run-z +z build,lint",
    "build": "run-z +z --then vite build",
    "clean": "run-z +z --then shx rm -rf dist target",
    "dev": "vite",
    "format": "run-z +z --then prettier-eslint --write --include-dot-files \"src/**/*.*\" \"*.{js,cjs,json,md}\"",
    "lint": "run-z +z --then eslint .",
    "site:publish": "run-z +z all --then gh-pages --dist dist --dotfiles",
    "start": "run-z +z build --then node ./build/server.js",
    "z": "run-z +cmd:vite,+cmd:eslint"
  }
}
