    "@frontmeans/namespace-aliaser": "^2.6.2",
    "@frontmeans/render-scheduler": "^1.8.1",
    "@frontmeans/style-producer": "^7.0.1",
    "@hatsy/hatsy": "^2.5.3",
    "@hatsy/log-z-request": "^3.0.0",
    "@hatsy/router": "^2.3.1",
    "@hatsy/serve-static": "^2.3.1",
    "@proc7ts/context-builder": "^7.0.1",
    "@proc7ts/context-values": "^7.1.0",
    "@proc7ts/delta-set": "^1.5.0",
    "@proc7ts/fun-events": "^10.5.2",
    "@proc7ts/logger": "^1.3.2",
    "@proc7ts/primitives": "^3.0.2",
    "@proc7ts/push-iterator": "^3.1.1",
